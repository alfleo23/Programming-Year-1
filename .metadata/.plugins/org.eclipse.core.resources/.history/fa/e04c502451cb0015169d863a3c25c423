
public class sequence {

	public static void main(String[] args) {
		
		sequenceMethod(1,12);

	} // end of main
	
	
	public static void sequenceMethod(int start, int end)
	{
		int count= 0;
		float countModule;
		int i;
		for (i=start; i<=end; i++)
		{
			count++;
			countModule= count % 3;
			if (countModule == 0 && i != 3)
			{
				System.out.print("*");
			}
			System.out.print(i);
		}
	}

}
