
public class sequence {

	public static void main(String[] args) {
		
		sequenceMethod(3,10);

	} // end of main
	
	
	public static void sequenceMethod(int start, int end)
	{
		int i;
		int count=0;
		for (i=start; i<=end; i++)
		{
			count++;
			if (count < 3)
			{
				System.out.print(start + " ");
				start= start +3;
			}
			if (count == 3)
			{
				System.out.print("*" + " ");
				start= start+3;
				count = 0;
			}
			
			
			
		}
	}

}
